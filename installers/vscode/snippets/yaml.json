	"[NetlifyCMS] Base Scaffold": {
		"prefix": "n-base",
		"body": [
			"locale: '${1:es}'",
			"media_folder: 'static/media' # Media files will be stored in the repo under static/media",
			"public_folder: '/media' # The src attribute for uploaded media will begin with /media",
			"",
			"site_url: 'https://$2' # URL to your published site",
			"logo_url: 'https://$2/media/primary-logo.png' #brand the CMS with your own logo",
			"",
			"backend:",
			"  name: 'git-gateway'",
			"  accept_roles: ['administrador', 'editor']",
			"  branch: 'staging'",
			"  commit_messages:",
			"    create: 'cms({{collection}}): create {{path}}'",
			"    update: 'cms({{collection}}): update {{path}}'",
			"    delete: 'cms({{collection}}): delete {{path}}'",
			"    uploadMedia: 'cms(media): upload {{path}}'",
			"    deleteMedia: 'cms(media): delete {{path}}'",
			"  # squash_merges: true",
			"",
			"publish_mode: 'editorial_workflow'",
			"",
			"slug:",
			"  encoding: 'ascii'",
			"  clean_accents: true",
			"",
			"collections:",
			"  [",
			"    $0",
			"  ]"
		],
		"description": "[NetlifyCMS] Base Scaffold"
	},
	//
	// Spanish Snippets
	//
	"[NetlifyCMS/Collection] Template": {
		"prefix": "n-es-template-*",
		"body": [
			"{",
			"  name: '${1:collection-slug}',",
			"  label: '${2:collection-label}',",
			"  description: '$3',",
			"  identifier_field: 'meta.title',",
			"  preview_path: '$1/{{slug}}',",
			"  delete: false,",
			"  editor: { preview: false },",
			"  folder: 'content/$1/detail',",
			"  fields:",
			"    [",
			"      {",
			"        name: 'meta',",
			"        label: 'Meta',",
			"        widget: 'object',",
			"        fields:",
			"          [",
			"            {",
			"              name: 'title',",
			"              label: 'Título',",
			"              widget: 'string',",
			"              hint: 'Título de la página',",
			"              pattern:",
			"                [",
			"                  '.{1,30}',",
			"                  'El título no debería tener más de 30 caracteres',",
			"                ],",
			"            },",
			"            {",
			"              name: 'description',",
			"              label: 'Descripción',",
			"              widget: 'string',",
			"              hint: 'Descripción de la página que aparecerá en los resultados de las búsquedas',",
			"              pattern:",
			"                [",
			"                  '.{1,150}',",
			"                  'La descripción no debería tener más de 150 caracteres',",
			"                ],",
			"            },",
			"          ],",
			"      },",
			"      {",
			"        name: 'cover',",
			"        label: 'Portada',",
			"        widget: 'object',",
			"        fields:",
			"          [",
			"            {",
			"              name: 'coverImage',",
			"              label: 'Imagen',",
			"              widget: 'object',",
			"              fields:",
			"                [",
			"                  {",
			"                    name: 'image',",
			"                    label: 'Archivo',",
			"                    widget: 'image',",
			"                    media_library:",
			"                      { config: { max_file_size: 5242880 } },",
			"                    hint: 'Imagen de portada de la página. Ha de pesar menos de 5MB, y tener dimensiones mínimas de 300x157 y máximas de 4096x4096 pixels',",
			"                  },",
			"                  {",
			"                    name: 'description',",
			"                    label: 'Descripción',",
			"                    widget: 'string',",
			"                    hint: 'Breve descripción de la imagen de portada',",
			"                  },",
			"                ],",
			"            },",
			"            {",
			"              name: 'heading',",
			"              label: 'Cabecera',",
			"              widget: 'string',",
			"              hint: 'Frase que encabeza el contenido de la página.',",
			"            },",
			"          ],",
			"      },$0",
			"    ],",
			"},",
		],
		"description": "[NetlifyCMS/Collection] Template"
	},
